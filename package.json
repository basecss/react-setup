{
  "name": "react-setup",
  "version": "1.0.0",
  "description": "React + Webpack + Babel + Jest + Enzyme setup. A simple react boilerplate.",
  "main": "src/index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/config/",
      "/public/"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack-dev.config.js --debug --progress --display-error-details",
    "build": "rimraf ./build && webpack --config config/webpack-build.config.js --progress",
    "serve": "http-server ./build -p 8083",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basecss/react-setup.git"
  },
  "keywords": [
    "React",
    "webpack",
    "babel",
    "jest",
    "enzyme"
  ],
  "author": "basecss <i@basecss.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basecss/react-setup/issues"
  },
  "homepage": "https://github.com/basecss/react-setup#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-jest": "^24.6.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-monitor": "^1.0.14"
  }
}
